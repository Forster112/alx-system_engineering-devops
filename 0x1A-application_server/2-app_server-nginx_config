server {
    listen 80;
    listen [::]:80 ipv6only=on;
    server_name 3.236.224.183;

    add_header X-Served-By $HOSTNAME;

    location /airbnb-onepage {
        proxy_pass http://127.0.0.1:5000/airbnb-onepage/;

    }

    error_page 404 /404.html;
    location /404 {
    	root /var/www/html;
    	internal;
    }
}
